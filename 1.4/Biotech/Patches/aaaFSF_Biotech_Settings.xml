				<caseTrue>
					<li Class="XmlExtensions.Setting.SetColor">
						<color>(163,177,188)</color>
					</li>
					<li Class="XmlExtensions.Setting.Numeric">
						<key>FSFPollutionPumpsPower</key>
						<defaultValue>200</defaultValue>
						<min>1</min>
						<max>10000</max>
						<label>Power Usage</label>
						<tKey>FSFTweaksPollutionPumpsPower</tKey>
					</li>
					<li Class="XmlExtensions.Setting.Numeric">
						<key>FSFPollutionPumpsRadius</key>
						<defaultValue>10</defaultValue>
						<min>1</min>
						<max>1000</max>
						<label>Radius</label>
						<tKey>FSFTweaksPollutionPumpsRadius</tKey>
					</li>
					<li Class="XmlExtensions.Setting.Numeric">
						<key>FSFPollutionPumpsWaste</key>
						<defaultValue>6</defaultValue>
						<min>1</min>
						<max>10000</max>
