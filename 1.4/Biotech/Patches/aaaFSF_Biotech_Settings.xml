				<label>More Polux Seeds On Traders</label>
				<key>FSFMoreTraderItemsPoluxSeedOn</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksMoreTraderItemsPoluxSeedTitle</tKey>
