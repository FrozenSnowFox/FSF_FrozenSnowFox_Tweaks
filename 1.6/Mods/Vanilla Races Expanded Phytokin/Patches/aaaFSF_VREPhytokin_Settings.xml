<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	
		<Operation Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				
			<li Class="PatchOperationAdd">
				<xpath>/Defs/XmlExtensions.SettingsMenuDef[defName = "FrozenSnowFoxTweaksSettings"]/settings/li[tag="FrozenSnowFoxTweaksVREPhytokin"]/settings</xpath>
				<value>

			
						
			<li Class="XmlExtensions.Setting.CollapsibleSettings">
				<label>Vanilla Races Expanded Phytokin Tweaks</label>
				<headerFont>Medium</headerFont>
				<defaultState>Closed</defaultState>
				<anchor>Middle</anchor>
				<tKey>FSFTweaksVREPhytokinTweaksTitle</tKey>
				<settings>
					
			<li Class="XmlExtensions.Setting.GapLine">
			</li>
			
			<!-- No Workspeed Malus On Sap Blood -->
			
			<li Class="XmlExtensions.Setting.Checkbox">
				<label>No Workspeed Malus On Sap Blood</label>
				<key>FSFVREPhytokinSapBloodNoWorkspeedMalus</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksVREPhytokinSapBloodNoWorkspeedMalusTitle</tKey>
			</li>
			<li Class="XmlExtensions.Setting.SetColor">
				<color>(163,177,188)</color>
			</li>
			<li Class="XmlExtensions.Setting.Text">
				<text>Removes the work speed malus on the Sap Blood gene and lowers the gene complexity from -2 to -1.</text>
				<tKey>FSFTweaksVREPhytokinSapBloodNoWorkspeedMalusDesc</tKey>
			</li>
			<li Class="XmlExtensions.Setting.SetColor"/>
			
			<li Class="XmlExtensions.Setting.GapLine">
			</li>
					
				</settings>
			</li>
					
				</value>
			</li>
				
			</operations>
		</Operation>
	
</Patch>