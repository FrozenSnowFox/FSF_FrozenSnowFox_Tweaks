<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	
		<Operation Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				
			<li Class="PatchOperationAdd">
				<xpath>/Defs/XmlExtensions.SettingsMenuDef[defName = "FrozenSnowFoxTweaksSettings"]/settings/li[tag="FrozenSnowFoxTweaksVanillaPsycastsExpanded"]/settings</xpath>
				<value>

			
						
			<li Class="XmlExtensions.Setting.Text">
				<text>Vanilla Psycasts Expanded Tweaks</text>
				<font>Medium</font>
				<anchor>Middle</anchor>
				<tKey>FSFTweaksVanillaPsycastsExpandedTweaksTitle</tKey>
			</li>
			<li Class="XmlExtensions.Setting.GapLine">
			</li>
					
			<!-- Psycast Tweaks -->

			<li Class="XmlExtensions.Setting.Text">
				<text>Psycast Tweaks</text>
				<tKey>FSFTweaksPsycastTweaksTitle</tKey>
			</li>
			<li Class="XmlExtensions.Setting.SetColor">
				<color>(163,177,188)</color>
			</li>
			<li Class="XmlExtensions.Setting.Text">
				<text>Allows you to individually enable a variety of tweaks for psycasts.  These changes may be for changing targeting parameters or changing how specific psycasts function.</text>
				<tKey>FSFTweaksPsycastTweaksDesc</tKey>
			</li>
			<li Class="XmlExtensions.Setting.SetColor"/>
			<li Class="XmlExtensions.Setting.SetColor">
				<color>(163,177,188)</color>
			</li>
			<li Class="XmlExtensions.Setting.Checkbox">
				<label>Allow Self Targeting and Pawn Targetting where it makes sense.</label>
				<key>FSFPsycastTweaksTargetChanges</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksPsycastTweaksTargetChanges</tKey>
			</li>
			<li Class="XmlExtensions.Setting.Checkbox">
				<label>Killskip Works Through Walls.</label>
				<key>FSFPsycastTweaksKillskip</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksPsycastTweaksKillskip</tKey>
			</li>
			<li Class="XmlExtensions.Setting.Checkbox">
				<label>Power Leap Works Through Walls.</label>
				<key>FSFPsycastTweaksPowerLeap</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksPsycastTweaksPowerLeap</tKey>
			</li>
			<li Class="XmlExtensions.Setting.Checkbox">
				<label>Skip Works Through Walls.</label>
				<key>FSFPsycastTweaksSkip</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksPsycastTweaksSkip</tKey>
			</li>
			<li Class="XmlExtensions.Setting.Checkbox">
				<label>Shield Psycasts Allow Outgoing Projectiles.</label>
				<key>FSFPsycastTweaksSkipshield</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksPsycastTweaksSkipshield</tKey>
			</li>
			<li Class="XmlExtensions.Setting.Checkbox">
				<label>Solar Pinhole Provides Sunlight.</label>
				<key>FSFPsycastTweaksSolarPinhole</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksPsycastTweaksSolarPinhole</tKey>
			</li>
			<li Class="XmlExtensions.Setting.SetColor"/>
			<li Class="XmlExtensions.Setting.GapLine">
				<spacing>12</spacing>
			</li>
			
			<!-- Unlock Psycast Trees -->
			
			<li Class="XmlExtensions.Setting.Checkbox">
				<label>Unlock Psycast Trees</label>
				<key>FSFUnlockPsycastTrees</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksUnlockPsycastTreesTitle</tKey>
			</li>
			<li Class="XmlExtensions.Setting.SetColor">
				<color>(163,177,188)</color>
			</li>
			<li Class="XmlExtensions.Setting.Text">
				<text>Unlocks the locked Psycast trees allowing anyone to use them.</text>
				<tKey>FSFTweaksUnlockPsycastTreesDesc</tKey>
			</li>
			<li Class="XmlExtensions.Setting.SetColor"/>
			<li Class="XmlExtensions.Setting.GapLine">
			</li>					
						
				
					
				</value>
			</li>
				
			</operations>
		</Operation>
	
</Patch>