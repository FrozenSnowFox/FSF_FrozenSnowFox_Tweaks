<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	
		<Operation Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				
			<li Class="PatchOperationAdd">
				<xpath>/Defs/XmlExtensions.SettingsMenuDef[defName = "FrozenSnowFoxTweaksSettings"]/settings/li[tag="FrozenSnowFoxTweaksVanillaExpandedMemes"]/settings</xpath>
				<value>

			
						
			<li Class="XmlExtensions.Setting.Text">
				<text>Vanilla Ideology Expanded - Memes and Structures Tweaks</text>
				<font>Medium</font>
				<anchor>Middle</anchor>
				<tKey>FSFTweaksVanillaExpandedMemesTweaksTitle</tKey>
			</li>
			<li Class="XmlExtensions.Setting.GapLine">
			</li>
			
			<!-- Unlock Fishing Traps -->
			
			<li Class="XmlExtensions.Setting.Checkbox">
				<label>Unlock Fishing Traps</label>
				<key>FSFUnlockFishingTraps</key>
				<defaultValue>false</defaultValue>
				<tKey>FSFTweaksUnlockFishingTrapsTitle</tKey>
			</li>
			<li Class="XmlExtensions.Setting.SetColor">
				<color>(163,177,188)</color>
			</li>
