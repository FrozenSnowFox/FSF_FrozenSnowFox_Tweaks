<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>FrozenSnowFox.FrozenSnowFoxTweaks</modId>
		<key>FSFAddXenotypesVenerated</key>
		<defaultValue>false</defaultValue>
		<caseTrue>
			
		<Operation Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				
			<!-- Venerated Xenotype -->
				
					<!--
							<li MayRequire="vanillaracesexpanded.archon">VRE_EarlyMaturity</li>
							<li MayRequire="vanillaracesexpanded.saurid">VRESaurids_Pheromones</li>
					-->
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs</xpath>
				<value>
					<XenotypeDef>
						<defName>FSFVenerated</defName>
						<label>Venerated Xenotype</label>
						<description>A basic Xenotype designed to be used with the mod Venerated Xenotype Flexibility.</description>
						<descriptionShort>A basic Xenotype designed to be used with the mod Venerated Xenotype Flexibility.</descriptionShort>
						<iconPath>UI/Icons/Xenotypes/Other</iconPath>
						<inheritable>true</inheritable>
						<genes>
							<li>ArchiteMetabolism</li>
							<li>DarkVision</li>
							<li>Learning_Fast</li>
							<li>XenogermReimplanter</li>
						</genes>
					</XenotypeDef>
				</value>
			</li>
				
			</operations>
		</Operation>
			
		</caseTrue>
	</Operation>

</Patch>